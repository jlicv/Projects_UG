@techreport{visvikis_investigation_2002,
	address = {Rochester, NY},
	type = {{SSRN} {Scholarly} {Paper}},
	title = {An {Investigation} of the {Lead}-{Lag} {Relationship} in {Returns} and {Volatility} between {Cash} and {Stack} {Index} {Futures}: {The} {Case} of {Greece}},
