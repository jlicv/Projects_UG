@techreport{kim_dynamic_2016,
	address = {Rochester, NY},
	type = {{SSRN} {Scholarly} {Paper}},
	title = {Dynamic {Conditional} {Correlations} between {Chinese} {Sector} {Returns} and the {S}\&{P}500 {Index}: {An} {Interpretation} {Based} on {Investment} {Shocks}},
	shorttitle = {Dynamic {Conditional} {Correlations} between {Chinese} {Sector} {Returns} and the {S}\&{P}500 {Index}},
	url = {http://papers.ssrn.com/abstract=2713602},
	abstract = {This paper examines the dynamic conditional correlations between the Chinese sector returns and the S\&P500 index returns and offers an interpretation for the heterogeneity of sector-level return correlations. Using a sample of 12 Chinese sectors for the period of 2006-2014, we find that their conditional correlations with the S\&P500 index vary significantly across sectors and across the two crises, namely, the 2008-2009 Global Financial Crisis and the 2010-2011 European Debt Crisis. To provide an economic interpretation for the dynamic conditional correlations, we lean on the general equilibrium model, as suggested by Papanikolaou (2011), in which the heterogeneity of sector-level conditional correlations can be associated with the heterogeneous sensitivities to investment shocks. We finally verify our interpretation by investigating the relationship between sector-level conditional correlations and industry-specific investment opportunities. Our main finding is that investment opportunities, as proxied by book-to-market ratio, capital expenditure, long-term debt ratio, growth rate of industry size, and Tobin's Q, are significantly associated with the magnitude of the dynamic conditional correlations. This paper thereby advances our understanding of industry heterogeneities from the perspective of industries' responses to investment shocks.},
	number = {ID 2713602},
	urldate = {2016-03-28},
	institution = {Social Science Research Network},
	author = {Kim, Myeong Hyeon and Sun, Lingxia},
	month = jan,
	year = {2016},
	keywords = {Dynamic conditional correlation, Growth opportunity, Investment-specific shock, Sector portfolios},
	file = {Snapshot:files/362/papers.html:text/html;SSRN-id2713602.pdf:files/374/SSRN-id2713602.pdf:application/pdf}
